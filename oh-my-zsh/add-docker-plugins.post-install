#!/usr/bin/env bash

# Docker
mkdir -p $HOME/.oh-my-zsh/plugins/docker
curl -L https://raw.githubusercontent.com/docker/docker/master/contrib/completion/zsh/_docker > $HOME/.oh-my-zsh/plugins/docker/_docker

# Compose
mkdir -p $HOME/.oh-my-zsh/plugins/docker-compose
curl -L https://raw.githubusercontent.com/docker/compose/master/contrib/completion/zsh/_docker-compose > $HOME/.oh-my-zsh/plugins/docker-compose/_docker-compose
